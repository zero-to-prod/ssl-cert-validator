{
  "type": "library",
  "name": "zero-to-prod/ssl-cert-validator",
  "description": "Fetch, validate, and verify SSL certificates.",
  "keywords": [
    "zero-to-prod",
    "ssl-certificate",
    "ssl-cert-validator"
  ],
  "homepage": "https://github.com/zero-to-prod/ssl-cert-validator",
  "support": {
    "email": "dave0016@gmail.com",
    "issues": "https://github.com/zero-to-prod/ssl-cert-validator/issues",
    "source": "https://github.com/zero-to-prod/ssl-cert-validator",
    "docs": "https://zero-to-prod.github.io/ssl-cert-validator/",
    "security": "https://github.com/zero-to-prod/ssl-cert-validator/blob/main/SECURITY.md"
  },
  "license": "MIT",
  "readme": "./README.md",
  "authors": [
    {
      "name": "David Smith",
      "email": "dave0016@gmail.com",
      "homepage": "https://davidsmith.blog/",
      "role": "maintainer"
    }
  ],
  "funding": [
    {
      "type": "GitHub",
      "url": "https://github.com/sponsors/zero-to-prod"
    }
  ],
  "scripts": {
    "test-all": [
      "sh test.sh"
    ],
    "docker": [
      "docker compose run --rm "
    ]
  },
  "scripts-descriptions": {
    "test-all": "tests all php versions",
    "docker": "runs a docker container"
  },
  "require": {
    "php": ">=7.1",
    "zero-to-prod/url": "^71.0",
    "zero-to-prod/data-model": "^71.0",
    "zero-to-prod/transformable": "^71.0",
    "zero-to-prod/stream-socket": "^71.0",
    "zero-to-prod/stream-context": "^71.0",
    "zero-to-prod/dynamic-setter": "^71.0",
    "ext-openssl": "*",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "Zerotoprod\\SslCertValidator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "ext-json": "*"
  }
