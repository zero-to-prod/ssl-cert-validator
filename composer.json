{
  "type": "library",
  "name": "zero-to-prod/ssl-cert-validator",
  "description": "Fetch, validate, and verify SSL certificates.",
  "keywords": [
    "zero-to-prod",
    "ssl-certificate",
    "ssl-cert-validator"
  ],
  "homepage": "https://github.com/zero-to-prod/ssl-cert-validator",
  "license": "MIT",
  "authors": [
    {
      "name": "David Smith",
      "email": "dave0016@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "zero-to-prod/url": "^71.0",
    "zero-to-prod/data-model": "^71.0",
    "zero-to-prod/transformable": "^71.0",
    "zero-to-prod/stream-socket": "^71.0",
    "zero-to-prod/stream-context": "^71.0",
    "zero-to-prod/dynamic-setter": "^71.0",
    "ext-openssl": "*",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "Zerotoprod\\SslCertValidator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "ext-json": "*"
  }
}
